{
  "version": 2,
  "name": "skillmatrix",
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node-server"
    },
    { "src": "package.json", "use": "@now/next", "config": { "distDir": "build" } },
    {
      "src": "static/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/*",
      "dest": "../pages/index.js"
    }
  ]
}